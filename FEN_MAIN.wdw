     p_codes :
      -
        code : |1-
         
         sFichier est une chaine
         
         // Pas de document courant
         si CFI_DOCUMENT..FenêtreSource = "" alors retour
         
         // On a sauvé le document ouvert dans les ..Note du champ
         // Si ..Note est vide, c'est un document vierge
         si CFI_DOCUMENT..note = "" ALORS
         	
         	// Ouvre le sélecteur de fichiers
         	sFichier = fSélecteur("", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Document graphique (*.WDDOC)" + TAB + "*.WDDOC", "WDDOC", fselCrée + fselExiste)
         	si sFichier = "" ALORS
         		retour
         	FIN
         	
         sinon
         	
         	// Fichier ouvert
         	sFichier = CFI_DOCUMENT..Note	
         FIN
         
         sablier(vrai)
         
         // Sauvegarde complète
         si FenEdSauve(CFI_DOCUMENT, sFichier, feModeComplet) ALORS
         	
         	// Charge la fenêtre interne
         	ChangeFenêtreSource(CFI_DOCUMENT, FI_ESPACE_TRAVAIL, "", sFichier)	
         	
         	// Titre
         	MaFenêtre..Titre = ProjetInfo(piNomProjet) + " - " + fExtraitChemin(sFichier, fFichier + fExtension)
         		
         	// Conserve le nom du fichier pour la prochaine sauvegarde
         	CFI_DOCUMENT..Note = sFichier
         	CFI_DOCUMENT..Visible = Vrai
         		
         	// On prévient que l'enregistrement a réussi	
         	ToastAffiche("Document enregistré", toastCourt, cvMilieu, chCentre)
         sinon
         	erreur()
         FIN
         
         sablier(faux)
