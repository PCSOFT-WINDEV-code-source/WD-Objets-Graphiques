        code : |1-
         
         sFichier est une chaine
         oSource est une image
         oChamp est un champ
         stInfoObjet est un ObjetChamp
         nLargeur, nHauteur sont des entiers
         
         // Pas de document courant
         SI CFI_DOCUMENT..FenêtreSource = "" ALORS RETOUR
         
         // Ouvre le sélecteur d'images
         sFichier = fSélecteurImage(frepexe, "", "Sélectionnez une image...")
         si sFichier = "" alors retour
         
         // Décrit l'objet à créer
         stInfoObjet.Nom = "IMG_IMAGE"
         stInfoObjet.Type = typImage
         stInfoObjet.Propriétés["X"] = CFI_DOCUMENT..X + 6
         stInfoObjet.Propriétés["Y"] = CFI_DOCUMENT..Y + 6
         stInfoObjet.Propriétés["Valeur"] = sFichier
         stInfoObjet.Propriétés["CouleurFond"] = transparent
         stInfoObjet.Propriétés["ImageMode"] = imgHSAC + imgHQ	// Mode Homothétique Centré Sans Agrandissement + Haute Qualité
         stInfoObjet.Propriétés["Visible"] = vrai				// Le champ est visible et actif
         stInfoObjet.Propriétés["Animation"] = vrai				// Le champ est animé
         
         // Charge l'image
         oSource = sFichier 
         
         // Dimensions réelles de l'image
         nLargeur = oSource..Largeur
         nHauteur = oSource..Hauteur
         
         // Calcule l'homothétie nécessaire pour afficher l'image dans le champ en entier
         Homothétie(nLargeur, nHauteur, 100, 100, vrai)
         
         // Dimensions adaptées
         stInfoObjet.Propriétés["Largeur"] = nLargeur
         stInfoObjet.Propriétés["Hauteur"] = nHauteur
         
         // Demande au document de créer l'objet : avec gestion de l'UNDO
         oChamp <- CFI_DOCUMENT.MGRObjetCrée(stInfoObjet)
         
         // Autorise l'édition
         oChamp..FenEditionAutorisée = Vrai
