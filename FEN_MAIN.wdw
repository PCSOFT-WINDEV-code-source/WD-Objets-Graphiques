   -
     name : MAJEtatBoutons
     internal_properties : CAAAAAgAAAC3XIrAPU3dxPS3WxkDu16ghzngQkY3n//yPB69+saFvSexCXAjzcjwZ9qr/VJEnJMZ1hdqI56Te/uhwUjUNetJRbkbbmf2nzGQRwNuxHR+s/GYxdzobDQZWAVDZ0KDJAOh0LL4t5DXXHw3w7sTLQ9H0BZ6/Av9+FA/blrt/Ss4Ow==
     procedure_id : 1609326096949075393
     type_code : 14
     code : |1-
      // Résumé : Mise à jour de l'état des boutons
      // Syntaxe :
      //MAJEtatBoutons ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle sera répétée en boucle, en attendant 50 centièmes de seconde entre chaque appel
      // Un nouveau timer est créé pour chaque appel à la procédure
      // 
      
      PROCEDURE MAJEtatBoutons()
      
      	si Vide ALORS
      				
      		// Désactive tous les outils
      		OutilDésactive(BTN_ENREGISTRER, 
      						BTN_INS_IMAGE, 
      						BTN_INS_FORME, 
      						BTN_ANNULER, 
      						BTN_RETABLIR, 
      						BTN_ALIGNEMENT, 
      						BTN_STYLE, 
      						BTN_COPIER, 
      						BTN_COUPER, 
      						BTN_COLLER)		
      		
      	sinon
      		
      		// Active les outils
      		OutilActive(BTN_ENREGISTRER, 
      					BTN_INS_IMAGE, 
      					BTN_INS_FORME)
      				
      		// UNDO
      		si CFI_DOCUMENT.FaaundoPossible() ALORS
      			OutilActive(BTN_ANNULER)
      		sinon
      			OutilDésactive(BTN_ANNULER)			
      		FIN
      				
      		// REDO
      		si CFI_DOCUMENT.FaaRedoPossible() ALORS
      			OutilActive(BTN_RETABLIR)
      		sinon
      			OutilDésactive(BTN_RETABLIR)			
      		FIN
      		
      		// COLLER
      		si PressePapierFormat(CF_OBJETCHAMP) ALORS
      			OutilActive(BTN_COLLER)
      		sinon
      			OutilDésactive(BTN_COLLER)
      		FIN
      				
      		// ALIGNEMENT, COPIER/COUPER
      		si FenEdSelectOccurrence(CFI_DOCUMENT) > 0 ALORS
      			
      			// Si on a un objet sélectionné
      			SI FenEdSelectOccurrence(CFI_DOCUMENT) = 1 ALORS
      				
      				// Récupère l'objet sélectionné
      				oSelection est un champ <- FenEdSelect(CFI_DOCUMENT,1)
      				
      				// Si c'est une liaison
      				si oSelection <> null _et_ oSelection..type = typfil ALORS
      					
      					// Pas de style, alignement ou presse-papier possible
      					OutilDésactive(BTN_ALIGNEMENT, 
      									BTN_STYLE, 
      									BTN_COPIER, 
      									BTN_COUPER)
      									
      					retour
      				FIN
      			FIN
      			
      			
      			OutilActive(BTN_ALIGNEMENT, 
      						BTN_STYLE, 
      						BTN_COPIER, 
      						BTN_COUPER)
      			
      		sinon
      			OutilDésactive(BTN_ALIGNEMENT, 
      							BTN_STYLE, 
      							BTN_COPIER, 
      							BTN_COUPER)
      							
      			
      		FIN
      	FIN
      		
      		// Désactive un ou des champs (style, plutôt que ..Grisé)
      		procedure interne OutilDésactive(*)
      			pour i = 1 _a_ MesParamètres..NbReçus
      				si MesParamètres[i]..Etat <> Inactif alors
      					MesParamètres[i]..Etat = inactif
      					MesParamètres[i]..Opacité = 50
      				fin
      			fin
      		FIN
      	
      		// Active un ou des champs (style, plutôt que ..Grisé)
      		procedure interne OutilActive(*)
      			POUR i = 1 _a_ MesParamètres..NbReçus
      				SI MesParamètres[i]..Etat <> Actif ALORS
      					MesParamètres[i]..Etat = Actif
      					MesParamètres[i]..Opacité = 100
      				FIN
      			FIN
      		FIN
